{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/products/(.*)",
            "dest": "./index.js"
        },
        {
            "src": "/api/cart/(.*)",
            "dest": "./index.js"
        },
        {
            "src": "/(.*)",
            "dest": "./index.js"
        }
    ]
}